{
  "cli": {
    "version": ">= 0.6.0"
  },
  "build": {
    "development": {
      "releaseChannel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "RELEASE_CHANNEL": "development"
      }
    },
    "production": {
      "node": "20.8.0",
      "releaseChannel": "production",

      "env": {
        "RELEASE_CHANNEL": "production"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "node": "20.8.0",
      "releaseChannel": "production",
      "env": {
        "RELEASE_CHANNEL": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {},
      "ios": {}
    }
  }
}
