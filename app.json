{
  "expo": {
    "name": "umamiapp",
    "description": "Hamburgueseria Umami",
    "slug": "umami-burger",
    "version": "1.2.0",
    "owner": "ifmael",
    "orientation": "portrait",
    "icon": "./src/assets/ios.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/android.png",
        "backgroundColor": "#FFF"
      },
      "userInterfaceStyle": "light",
      "package": "com.ifmael.umami",
      "versionCode": 440010200,
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ]
    },
    "androidStatusBar": {
      "barStyle": "dark-content"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "bundleIdentifier": "com.ifmaeles.umami",
      "buildNumber": "1.2.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Para enviar tu pedido a casa."
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "primaryColor": "#FFFFFF"
  }
}
