{
  "version": "2.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "lint:fix": "eslint \"**/*.{js,jsx}\" --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "release:patch": "standard-version -r patch",
    "release:minor": "standard-version -r minor",
    "release:major": "standard-version -r major",
    "release:simulation": "standard-version --dry-run",
    "publish:production": "expo publish --release-channel production",
    "eas:build:production": "eas build --profile production",
    "eas:submit:all": "eas submit -p all"
  },
  "dependencies": {
    "@apollo/client": "~3.7.3",
    "@expo-google-fonts/comfortaa": "^0.2.2",
    "@expo-google-fonts/inter": "^0.2.2",
    "@expo/webpack-config": "^0.17.2",
    "@react-native-async-storage/async-storage": "~1.17.11",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/drawer": "^6.5.6",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@sentry/react-native": "4.9.0",
    "dayjs": "^1.11.7",
    "dotenv": "^10.0.0",
    "expo": "^47.0.12",
    "expo-app-loading": "~2.1.1",
    "expo-application": "~5.0.1",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-device": "~5.0.0",
    "expo-font": "~11.0.1",
    "expo-location": "~15.0.1",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "graphql": "^16.6.0",
    "metro-core": "^0.73.7",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-native": "0.70.5",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-google-places-autocomplete": "https://github.com/ifmael/react-native-google-places-autocomplete",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.2",
    "react-native-super-grid": "^5.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "0.18.10",
    "react-native-wizard": "^2.1.1",
    "sentry-expo": "~6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0",
    "standard-version-expo": "^1.0.3"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint \"**/*.{js,jsx}\""
    ],
    "*.+(js|jsx|json)": [
      "prettier --write \"**/*.+(js|jsx|json|css|md)\""
    ]
  },
  "resolutions": {
    "promise": "^8.0.3"
  }
}
